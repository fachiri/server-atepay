{
  "name": "server-atepay",
  "version": "1.0.0",
  "description": "Node.js Express Server for ATEPAY app",
  "main": "server.js",
  "scripts": {
    "start": "nodemon server.js",
    "sync": "node app/db/sync.js",
    "alter": "node app/db/alter.js",
    "seed": "node app/db/seed.js",
    "truncate": "node app/db/truncate.js",
